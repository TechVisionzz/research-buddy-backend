{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "Collection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "collections"
    },
    "trash": {
      "type": "boolean",
      "default": false
    },
    "references": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reference.reference",
      "mappedBy": "collection"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::collection.collection",
      "inversedBy": "collections"
    },
    "collections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::collection.collection",
      "mappedBy": "parent"
    }
  }
